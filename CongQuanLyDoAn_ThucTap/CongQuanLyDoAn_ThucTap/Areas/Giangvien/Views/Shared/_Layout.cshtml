<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Trang chủ</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="" />
    <meta name="author" content="" />

    <!-- css -->
    <link href="https://fonts.googleapis.com/css?family=Noto+Serif:400,400italic,700|Open+Sans:300,400,600,700" rel="stylesheet">
    <link href="/Assets/GiangVien/css/bootstrap.css" rel="stylesheet" />
    <link href="/Assets/GiangVien/css/bootstrap-responsive.css" rel="stylesheet" />
    <link href="/Assets/GiangVien/css/fancybox/jquery.fancybox.css" rel="stylesheet">
    <link href="/Assets/GiangVien/css/jcarousel.css" rel="stylesheet" />
    <link href="/Assets/GiangVien/css/flexslider.css" rel="stylesheet" />
    <link href="/Assets/GiangVien/css/style.css" rel="stylesheet" />
    <link href="~/Assets/GiangVien/css/overwrite.css" rel="stylesheet" />
    <!-- Theme skin -->
    <link href="/Assets/GiangVien/skins/default.css" rel="stylesheet" />
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/Assets/GiangVien/ico/apple-touch-icon-144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/Assets/GiangVien/ico/apple-touch-icon-114-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/Assets/GiangVien/ico/apple-touch-icon-72-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" href="/Assets/GiangVien/ico/apple-touch-icon-57-precomposed.png" />
    <link rel="shortcut icon" href="/Assets/GiangVien/ico/favicon.png" />

    <!-- =======================================================
      Theme Name: Flattern
      Theme URL: https://bootstrapmade.com/flattern-multipurpose-bootstrap-template/
      Author: BootstrapMade.com
      Author URL: https://bootstrapmade.com
    ======================================================= -->
</head>

<body>
    <div id="wrapper">
        <!-- start header -->

        <header>
            <div class="container">
                <div class="row nomargin">
                    <div class="headnav" style="margin:10px 0 0">
                        <p style="font:bolder">Chào giảng viên: <span style="color:red">@Session["TenGV"]</span>  </p>
                        <p style="display:none">@Session["MaGV"] @Session["Email"]</p>
                    </div>

                </div>

                <div class="row" style="margin-top: 30px;">
                    <div class="span4">
                        <div class="logo">
                            <a href="/Giangvien/GV_Home"><img src="~/Assets/GiangVien/img/logoDT-Hutech.png" alt="" class="logo" /></a>
                            <p style="color:mediumorchid">Kiến thức chỉ có được qua tư duy của con người</p>
                        </div>
                    </div>
                    <div class="span8">
                        <div class="navbar navbar-static-top">
                            <div class="navigation">
                                <nav>
                                    <ul class="nav topnav">
                                        <li class="active">
                                            <a href="/Giangvien/GV_Home">Trang chủ</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#">Nhóm DA-TT <i class="icon-angle-down"></i></a>
                                            <ul class="dropdown-menu">
                                                <li><a href="/Giangvien/GV_Nhom/DSNhom">Nhóm hướng dẫn</a></li>
                                                <li><a href="#">Thông báo</a></li>
                                            </ul>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#">Sinh viên <i class="icon-angle-down"></i></a>
                                            <ul class="dropdown-menu">
                                                <li><a href="#">Xem báo cáo</a></li>
                                                <li><a href="#">Liên hệ</a></li>
                                                <li><a href="#">Bảng điểm</a></li>
                                            </ul>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#">Tài Khoản <i class="icon-angle-down"></i></a>
                                            <ul class="dropdown-menu">
                                                <li><a href="/Giangvien/TaiKhoan/ThongTinCaNhan">Thông tin cá nhân</a></li>
                                                <li><a href="/Giangvien/TaiKhoan/DoiMatKhau">Đổi mật khẩu</a></li>
                                                <hr />
                                                <li><a href="@Url.Action("LogOut","GV_Login")">Đăng Xuất</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                            <!-- end navigation -->
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- end header -->
        <hr />
        <section id="content" style="padding-top: 0px">
            <div class="container">
                @if (TempData["AlertMessage"] != null)
                {
                    <div id="giangvien" class="alert @TempData["AlertType"]  hide" style="text-align:center">
                        <strong>@TempData["AlertMessage"]</strong>
                    </div>
                }
                <div style="font-family: Arial, Helvetica, sans-serif; min-height:304px">
                    @RenderBody()
                </div>

            </div>
        </section>
        <footer style="padding-top:0;">
            <div id="sub-footer">
                <div class="container">
                    <div class="row">
                        <div class="span6">
                            <div class="copyright">
                                <p>
                                    <span>&copy; Nguyễn Trung Tin - 16DTHA3 - HUTECH</span>
                                </p>
                                <div class="credits">
                                    <!--
                                      All the links in the footer should remain intact.
                                      You can delete the links only if you purchased the pro version.
                                      Licensing information: https://bootstrapmade.com/license/
                                      Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/buy/?theme=Flattern
                                    -->
                                    Designed by <a href="#">BootstrapMade</a>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <ul class="social-network">
                                <li><a href="#" data-placement="bottom" title="Facebook"><i class="icon-facebook icon-square"></i></a></li>
                                <li><a href="#" data-placement="bottom" title="Twitter"><i class="icon-twitter icon-square"></i></a></li>
                                <li><a href="#" data-placement="bottom" title="Linkedin"><i class="icon-linkedin icon-square"></i></a></li>
                                <li><a href="#" data-placement="bottom" title="Pinterest"><i class="icon-pinterest icon-square"></i></a></li>
                                <li><a href="#" data-placement="bottom" title="Google plus"><i class="icon-google-plus icon-square"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <a href="#" class="scrollup"><i class="icon-chevron-up icon-square icon-32 active"></i></a>
    <!-- javascript
      ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="/Assets/GiangVien/js/jquery.js"></script>
    <script src="/Assets/GiangVien/js/jquery.easing.1.3.js"></script>
    <script src="/Assets/GiangVien/js/bootstrap.js"></script>
    <script src="/Assets/GiangVien/js/jcarousel/jquery.jcarousel.min.js"></script>
    <script src="/Assets/GiangVien/js/jquery.fancybox.pack.js"></script>
    <script src="/Assets/GiangVien/js/jquery.fancybox-media.js"></script>
    <script src="/Assets/GiangVien/js/google-code-prettify/prettify.js"></script>
    <script src="/Assets/GiangVien/js/portfolio/jquery.quicksand.js"></script>
    <script src="/Assets/GiangVien/js/portfolio/setting.js"></script>
    <script src="/Assets/GiangVien/js/jquery.flexslider.js"></script>
    <script src="/Assets/GiangVien/js/jquery.nivo.slider.js"></script>
    <script src="/Assets/GiangVien/js/modernizr.custom.js"></script>
    <script src="/Assets/GiangVien/js/jquery.ba-cond.min.js"></script>
    <script src="/Assets/GiangVien/js/jquery.slitslider.js"></script>
    <script src="/Assets/GiangVien/js/animate.js"></script>

    <!-- Template Custom JavaScript File -->
    <script src="/Assets/GiangVien/js/custom.js"></script>
    @Scripts.Render("~/bunfles/jquery")
    @Scripts.Render("~/bunfles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
